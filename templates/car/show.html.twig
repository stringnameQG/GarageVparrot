{% extends 'base.html.twig' %}

{% block title %}Car{% endblock %}

{% block body %}
    <h1>Car</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ car.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ car.name }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ car.price }}</td>
            </tr>
            <tr>
                <th>Killometering</th>
                <td>{{ car.killometering }}</td>
            </tr>
            <tr>
                <th>Circulation</th>
                <td>{{ car.circulation }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ car.brand }}</td>
            </tr>
            <tr>
                <th>Model</th>
                <td>{{ car.model }}</td>
            </tr>
            <tr>
                <th>Fuel</th>
                <td>{{ car.fuel }}</td>
            </tr>
            <tr>
                <th>Gearbox</th>
                <td>{{ car.gearbox }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ car.color }}</td>
            </tr>
            <tr>
                <th>Numberofdoors</th>
                <td>{{ car.numberofdoors }}</td>
            </tr>
            <tr>
                <th>Fiscalpower</th>
                <td>{{ car.fiscalpower }}</td>
            </tr>
            <tr>
                <th>Powerdin</th>
                <td>{{ car.powerdin }}</td>
            </tr>
            <tr>
                <th>Otherinfo</th>
                <td>{{ car.otherinfo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_car_index') }}">back to list</a>

    <a href="{{ path('app_car_edit', {'id': car.id}) }}">edit</a>

    {{ include('car/_delete_form.html.twig') }}
{% endblock %}
